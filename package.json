{
  "name": "azure-devops-extension-sample",
  "version": "1.0.0",
  "description": "Azure DevOps Extension Sample",
  "keywords": [
    "Azure DevOps",
    "Visual Studio Team Services"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cookbr/azure-devops-extension-sample.git"
  },
  "author": "Bryan.Cook <bryan.cook@energage.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cookbr/azure-devops-extension-sample/issues"
  },
  "homepage": "https://github.com/cookbr/azure-devops-extension-sample#readme",
  "main": "./src/index.tsx",
  "scripts": {
    "clean": "rimraf ./dist",
    "compile": "npm run clean && webpack --mode production",
    "compile:dev": "npm run clean && webpack --mode development",
    "build": "npm run compile",
    "build:dev": "npm run compile:dev && npm run postbuild",
    "postbuild": "npm run package-extension -- --rev-version",
    "package-extension": "tfx extension create --manifest-globs azure-devops-extension.json src/**/*.json",
    "publish-extension": "tfx extension publish --manifest-globs azure-devops-extension.json src/**/*.json"
  },
  "dependencies": {
    "azure-devops-extension-api": "^1.158.0",
    "azure-devops-extension-sdk": "^2.0.11",
    "azure-devops-ui": "^2.167.22",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@types/react": "^17.0.35",
    "@types/react-dom": "^17.0.11",
    "base64-inline-loader": "~2.0.1",
    "copy-webpack-plugin": "^10.0.0",
    "css-loader": "~6.5.1",
    "node-sass": "^6.0.1",
    "rimraf": "~3.0.2",
    "sass-loader": "~12.3.0",
    "style-loader": "~3.3.1",
    "tfx-cli": "^0.9.3",
    "ts-loader": "~9.2.6",
    "typescript": "^4.5.2",
    "vss-version": "^1.0.2",
    "webpack-cli": "^4.9.1"
  }
}