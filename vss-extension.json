{
  "manifestVersion": 1,
  "version": "1.0.17",
  "name": "My First Extension",
  "description": "A short description of my extension",
  "publisher": "bryan-cook",
  "demands": [
    "api-version/3.0"
  ],
  "categories": [
    "Azure Repos"
  ],
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "content": {
    "details": {
      "path": "README.md"
    }
  },
  "icons": {
    "default": "img/world.png"
  },
  "files": [
    {
      "path": "dist",
      "addressable": true
    },
    {
      "path": "img",
      "addressable": true
    }
  ],
  "contributions": [
    {
      "id": "sample-work-item-form-control",
      "type": "ms.vss-work-web.work-item-form-control",
      "description": "Sample Work Item Form Control",
      "targets": [
        "ms.vss-work-web.work-item-form"
      ],
      "properties": {
        "name": "Sample Work Item Form Control",
        "uri": "dist/WorkItemControl/index.html"
      }
    }
  ],
  "scopes": [
    "vso.work"
  ]
}